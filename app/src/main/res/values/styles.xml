<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ProgressBar" parent="Widget.AppCompat.ProgressBar">
        <item name="android:indeterminate">true</item>
        <item name="android:progressBarStyle">@style/Widget.AppCompat.ProgressBar.Horizontal</item>
    </style>


    <style name="EditTextSearch">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorHint">@color/colorGrayOslo</item>
        <item name="android:colorPrimary">@color/colorTransparent</item>
        <item name="android:colorPrimaryDark">@color/colorTransparent</item>
    </style>

</resources>
